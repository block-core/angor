<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:common="clr-namespace:AngorApp.UI.Controls.Common"
             xmlns:feerate="clr-namespace:AngorApp.UI.Controls.Feerate"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AngorApp.UI.Controls.Common.FeerateSelectionView"
             x:DataType="common:FeerateSelectionViewModel">
    <feerate:FeerateSelector
        Presets="{Binding Presets}"
        FeeCalculator="{Binding FeeCalculator}"
        Amount="{Binding Amount}"
        Feerate="{Binding Feerate}" />
</UserControl>