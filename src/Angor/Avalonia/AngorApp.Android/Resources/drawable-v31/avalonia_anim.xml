            android:width="128dp"
            android:height="128dp"
            android:viewportWidth="364.4"
            android:viewportHeight="336.7">
            <group
                android:name="wrapper"
                android:pivotX="182.2"
                android:pivotY="168.35"
                android:scaleX="0.5"
                android:scaleY="0.5">
                <group android:name="logo_group">
                    <!-- Main triangular outline -->
                    <path
                        android:name="main_outer"
                        android:pathData="M180.9,15.7L180.9,15.7c1,-1.8,3.6,-1.8,4.6,0c20.2,37,40.2,74.1,60.5,111.1c15.3,28.1,30.7,56.2,46,84.4c0.7,1.3,0.2,2.9,-1.1,3.5c-35.5,18.8,-71,37.4,-106.5,56.3c-0.8,0.4,-1.7,0.4,-2.4,0c-35.4,-18.9,-71,-37.5,-106.5,-56.3c-1.3,-0.7,-1.8,-2.3,-1.1,-3.5c5.7,-10.4,11.3,-20.9,17,-31.3c10,-18,19.7,-36.1,29.6,-54.1C141,89.2,160.9,52.4,180.9,15.7z"
                        android:fillColor="#00000000"
                        android:strokeColor="#022229"
                        android:strokeAlpha="0"
                        android:strokeWidth="16"/>
                    
                    <!-- Inner triangle cutout -->
                    <path
                        android:name="main_inner"
                        android:pathData="M182.1,125.3c-12.1,22.4,-24.2,44.7,-36.4,67c0,0,0,0.1,0,0.1c11.9,6.5,24.1,12.7,36,19.1c0.8,0.5,1.9,0.5,2.8,0c12,-6.5,24.1,-12.6,36.1,-19.1c0,0,0,0,0,0c-2.3,-3.8,-4.3,-7.9,-6.5,-11.8c-10,-18.4,-20,-36.8,-30,-55.2C183.8,124.5,182.6,124.5,182.1,125.3z"
                        android:fillColor="#00000000"
                        android:strokeColor="#022229"
                        android:strokeAlpha="0"
                        android:strokeWidth="16"/>
                    
                    <!-- Bottom left triangle outline -->
                    <path
                        android:name="bottom_left"
                        android:pathData="M55.4,246.3c1.4,-2.6,4.7,-3.5,7.3,-2.2c12.6,6.6,25.2,13.3,37.8,19.9c2.7,1.4,3.7,4.7,2.3,7.4c-2.3,4.2,-4.7,8.3,-6.9,12.6c-6.2,11.2,-12.3,22.6,-18.5,33.8c-1,1.7,-2.8,2.8,-4.7,2.8c-14,0.1,-28.1,0,-42.1,0c0,0,-8,0.1,-12.7,0.1c-1.5,0,-2.4,-1.5,-1.7,-2.8C22.1,307,43.2,268.1,55.4,246.3z"
                        android:fillColor="#00000000"
                        android:strokeColor="#022229"
                        android:strokeAlpha="0"
                        android:strokeWidth="16"/>
                    
                    <!-- Bottom right triangle outline -->
                    <path
                        android:name="bottom_right"
                        android:pathData="M263.7,265.3c14.2,-7.3,28.4,-14.8,42.6,-22.2c1.4,-0.7,3.1,-0.2,3.9,1.2c12.8,22.6,25,45.4,37.6,68.1c0.7,1.4,1.5,2.8,2.3,4.1c1.1,1.9,-0.3,4.3,-2.5,4.3c-18.3,0,-36.7,0,-55,0c-1,0,-1.9,-0.5,-2.5,-1.4c-1.7,-2.9,-3.2,-6,-4.9,-8.9c-6.4,-11.6,-12.8,-23.2,-19.1,-34.8c-1.1,-2.2,-2.5,-4.3,-3.6,-6.5C261.7,267.8,262.2,266,263.7,265.3z"
